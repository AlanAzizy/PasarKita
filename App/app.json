{
  "expo": {
    "name": "PasarKita",
    "slug": "PasarKita",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon-test.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon-test.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "width" : "100%"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon-test.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.yourappname",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      {
        "fonts" : ["./assets/fonts/Poppins-Regular.ttf"]
      }
      
      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "abb9cc7d-9cf0-4f08-ae3b-7b1edf0e2047"
      }
    }
  }
}
